<h1>Your Group Information</h1>


<table>
  <tr>
    <th>User</th>
    <th>Dob</th>
    <th>Title</th>
    <th>Telephone</th>
    <th>Group name</th>
    <th>Group type</th>
    <th>Street address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Checking number</th>
    <th>Routing number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @groups.each do |group| %>
  <tr>
    <td><%= group.user_id %></td>
    <td><%= group.dob %></td>
    <td><%= group.title %></td>
    <td><%= group.telephone %></td>
    <td><%= group.group_name %></td>
    <td><%= group.group_type %></td>
    <td><%= group.street_address %></td>
    <td><%= group.city %></td>
    <td><%= group.state %></td>
    <td><%= group.zip %></td>
    <td><%= group.checking_number %></td>
    <td><%= group.routing_number %></td>
    <td><%= link_to 'Show', group %></td>
    <td><%= link_to 'Edit', edit_group_path(group) %></td>
    <td><%= link_to 'Destroy', group, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Group', new_group_path %>
